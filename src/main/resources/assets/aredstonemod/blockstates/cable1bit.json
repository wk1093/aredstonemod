{
  "multipart": [
    {
      "apply": {
        "model": "aredstonemod:block/cable/node"
      }
    },
    {
      "when": {
        "north": "cable"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270
      }
    },
    {
      "when": {
        "east": "cable"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "south": "cable"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270,
        "y": 180
      }
    },
    {
      "when": {
        "west": "cable"
      },
      "apply": {
          "model": "aredstonemod:block/cable/connector",
          "x": 270,
          "y": 270
      }
    },
    {
      "when": {
        "up": "cable"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 180
      }
    },
    {
      "when": {
        "down": "cable"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector"
      }
    },
    {
      "when": {
        "north": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270
      }
    },
    {
      "when": {
        "east": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270,
        "y": 90
      }
    },
    {
      "when": {
        "south": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270,
        "y": 180
      }
    },
    {
      "when": {
        "west": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 270,
        "y": 270
      }
    },
    {
      "when": {
        "up": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector",
        "x": 180
      }
    },
    {
      "when": {
        "down": "repeater"
      },
      "apply": {
        "model": "aredstonemod:block/cable/connector"
      }
    }
